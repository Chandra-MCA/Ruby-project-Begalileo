    <div
      class="modal fade w-100"
      id="exampleModal"
      tabindex="-1"
      aria-labelledby="exampleModalLabel"
      aria-hidden="true"
    >
      <div class="modal-dialog">
        <div class="modal-content">
          <input type=hidden id="jobId">
          <div class="modal-header py-1">
            <h5
              class="modal-title"
              id="exampleModalLabel"
            >
              Job Application
            </h5>
            <button
              type="button"
              class="btn-close"
              data-bs-dismiss="modal"
              aria-label="Close"
            ></button>
          </div>
          <div class="modal-body">
            <div id="jd">
              <h4 id="jt"></h4>
              <div class="d-flex gap-2">
                <p class="jd-head">Department</p>
                <p id="department">-</p>
              </div>
              <div class="d-flex gap-2">
                <p class="jd-head">Job Type</p>
                <p id="job-type">-</p>
              </div>
              <div class="d-flex gap-2 expDiv">
                <p class="jd-head">Experience</p>
                <p id="exp">-</p>
              </div>
              <div class="d-flex gap-2">
                <p class="jd-head">Description</p>
                <p id="desc">-</p>
              </div>
              <p class="jd-head">Enter Your Details</p>
            </div>
            <div class="container-fluid mb-4">
              <form
                action="index.html"
                method="POST"
                target="_blank"
                enctype="multipart/form-data"
                class="d-flex flex-column"
                id="application"
              >
                <input
                  autocomplete="off"
                  id="name"
                  class="mt-4 form-control"
                  type="text"
                  name="name"
                  placeholder="Your Full Name"
                  required
                  maxlength="35"
                  onkeypress="return ((event.charCode >= 65 && event.charCode <= 90) || (event.charCode >= 97 && event.charCode <= 122) || (event.charCode == 32))"
                />

                <input
                  autocomplete="off"
                  id="email"
                  class="form-control"
                  type="email"
                  name="email"
                  placeholder="Your Email"
                  required
                />

                <input
                  autocomplete="off"
                  id="experience"
                  class="form-control"
                  type="number"
                  name="experience"
                  placeholder="Your Working Experience(years)"
                  required
                />

                <input
                  autocomplete="off"
                  class="form-control"
                  type="file"
                  id="pdf"
                  name="resume"
                  placeholder="Yout Resume"
                  required
                  accept=".doc,.docx,.pdf"
                />
                <span class="invalid-feedback"
                  >File Size Exceeded ! Upload file below 5MB</span
                >
                <span
                  class="invalid-feedback2 text-danger"
                  style="display: none"
                  >Invalid file type</span
                >

                <textarea
                  autocomplete="off"
                  class="form-control"
                  name="randomtext"
                  id="text"
                  maxlength="200"
                  rows="5"
                  placeholder="Why do you want to join with us ?"
                  required
                ></textarea>
                <span class="label label-default" id="count_message"></span>

                <button class="mybtn mt-4 form-control" id="btn" type="submit" 
                style="background: linear-gradient(270deg, #2fb8bb 0%, #97da92 100%);
                      box-shadow: 2px 4px 8px rgba(3,128,155,0.2);
                      color: #fff;
                      font-weight: bold;
                      border: none;
                      font-size: 16px;
                      font-weight: 500;
                      color: #fff;
                      font-family: 'Montserrat';
                      padding: 16px 32px;
                      border-radius: 44px;
                      margin: 20px auto 0 auto;
                      cursor: pointer;
                      height: auto;
                      width: auto;"
                >
                  Apply For Job
                </button>
              </form>
            </div>
          </div>
          <div class="modal-footer py-1">
            <button
