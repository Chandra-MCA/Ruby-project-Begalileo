  let jobsArr
  function search() {
    console.log("btn click", document.querySelector(".form-select").value);
    let department_id = document.querySelector(".form-select").value;
    let keyword = document.querySelector("#input-search").value

    if(department_id == "" && keyword == ""){
      // alert("Please enter valid input")
      $("#searchErr").show()
      return
    }
    
    $("#searchErr").hide()

    $.ajax({
            type: 'get',
            url: '/careers/show_job_details',
            data: {
                department_id: department_id,
                keyword:keyword
            },
            success: function(data) {
              console.log(data)
              let jobDiv = document.getElementById("search-result");
              jobDiv.innerHTML = "";

              if (data.status == 0) {
                // document.querySelector(".notresult").classList.remove("d-none")
                jobDiv.innerHTML = "<p class='notResult'>Selected job posting not available. Please check after some time !</p>";
                return;
              }

              jobsArr = data.details

              data.details.forEach((elem,index) => {
                let job = `<div class="job d-flex">
                              <div class="col-8 col-md-7 col-lg-9">
                                <h5 class="jobTitle">${elem.job_title}</h5>
                                <p class="m-0 fw-bold jobType">${elem.job_type}</p>
                              </div>
                              <div class="col-4 col-md-5 col-lg-3 d-flex justify-content-center align-items-center">
                                <button class="applyBtn" onclick='OpenBootstrapPopup(${index})' >Apply</button>
                              </div>
                            </div>`;
                jobDiv.innerHTML += job;
              });
            },
            error: function (error) {
                console.log(error)
              }
        });
